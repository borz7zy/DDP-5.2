                        format(string, sizeof(string), "~b~Speed: ~y~%d", GetPlayerSpeed(i));
                        PlayerTextDrawSetString(g, spec_TD[g], string);
                    }
                }
            }
        }
    }
    if (polsec < 2) polsec++;
