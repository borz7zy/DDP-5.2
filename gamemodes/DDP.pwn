        if (GetPVarInt(playerid, "MnMode") == 1)
        {
            GameTextForPlayer(playerid, "~y~Press ~G~ALT", 1000, 1);
        }
        else
        {
            GameTextForPlayer(playerid, "~y~Press ~G~Y", 1000, 1);
        }
    }
    return 1;
