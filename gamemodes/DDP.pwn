flags:dice(CMD_SPAWNED)
CMD:dice(playerid, params[])
{
    if (AI[playerid][aSpectateID] != INVALID_PLAYER_ID) return 1;
    new playerto, sum, iplayer[MAX_PLAYER_NAME], tplayer[MAX_PLAYER_NAME], strmb[256];
    if (GetPlayerInterior(playerid) == 10)
    {
        if (FloodStop[playerid] == 1) return SendClientMessage(playerid, COLOR_RED, ""NS" Вы слишком часто играете в кости! Подождите пару секунд.");
        if (PlayerToPoint(40.0, playerid, 1989.8429, 1017.4302, 994.8906))
        {
            if (sscanf(params, "dd", playerto, sum)) return SendClientMessage(playerid, COLOR_RED, ""NS" {FFFFFF}Чтобы начать игру, введите {FFA500}/dice [ID игрока] [сумма]!");
            {
                if (playerid == playerto) return SendClientMessage(playerid, COLOR_RED, ""NS" Вы не можете играть с собой!");
                if (GetPVarInt(playerid, "PlMon") < 10000) return SendClientMessage(playerid, COLOR_RED, ""NS" У вас меньше $10000, Вы не можете играть в казино!");
                if (PlayerToPoint(50.0, playerto, 1990.8429, 1018.4302, 994.8806))
                {
                    if (GetPVarInt(playerto, "PlMon") < 10000) return SendClientMessage(playerid, COLOR_RED, ""NS" У игрока меньше $10000, он не можете играть с вами!");
                    if (GetPVarInt(playerto, "PlMon") < sum) return SendClientMessage(playerid, COLOR_RED, ""NS" У игрока недостаточно денег, он не можете играть с вами!");
                    if (sum >= 10000 && sum < 10000000)
                    {
                        GetPlayerName(playerid, iplayer, sizeof(iplayer));
                        GetPlayerName(playerto, tplayer, sizeof(tplayer));
                        if (DiceLock[playerto] == 0) //Если игрок не занят игрой, то
                        {
                            DiceLock[playerto] = 1; //Временно блокируем игрока на запросы для игры
