flags:leaders(CMD_SPAWNED)
CMD:leaders(playerid)
{
    new str[2048];
    format(str, sizeof(str), "{00FFFF}OnLine лидеры группы:{FFFFFF}\n");

    new c = 0;
    for (new i = 0; i < MAX_PLAYERS; i++)
    {
        if (IsPlayerConnected(i) && PlayerInfo[i][pGang] > 0 && PlayerInfo[i][pGangLvl] >= 6)
        {
            c++;
            format(str, sizeof(str), "%s{CCFF00}%s[%d] | {FFFF00}%s\n", str, PlayerInfo[i][pName], i, GangInfo[PlayerInfo[i][pGang]][gName]);
        }
    }

    if (c == 0)
    {
        format(str, sizeof(str), "%s{FF0000}На сервере нет онлайн лидеров групп!", str);
    }

    ShowPlayerDialog(playerid, DIALOG_ZERO, DIALOG_STYLE_MSGBOX, ">>[OnLine лидеры]<<", str, "Закрыть", "");
    return 1;
}

flags:test_cmd(CMD_ADMIN)
CMD:test_cmd(playerid)
{
    if (AI[playerid][aLevel] < 4) return 1;
    else SendClientMessage(playerid, COLOR_PURPLE, "Проверка пройдена!");
    return 1;
}
alias:test_cmd("tcmd")