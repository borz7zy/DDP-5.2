    if (GetPlayerVehicleID(playerid) == fire_tc[0] || GetPlayerVehicleID(playerid) == fire_tc[1] || GetPlayerVehicleID(playerid) == fire_tc[2])
    {
        if (job[playerid] == 2)
        {
            SetVehicleToRespawn(GetPlayerVehicleID(playerid));
            DestroyObject(object_fire[playerid]);
            DisablePlayerRaceCheckpoint(playerid);
            job[playerid] = 0;
            SendClientMessage(playerid, COLOR_VIOLET, "[ПОЖАРНИК]: {FFFFFF}Вы уволены с пожарника после смерти{CCFF00}!");
            SetPlayerSkin(playerid, PlayerInfo[playerid][pSkin]);
        }
    }
    R_Vehicle[playerid] = VehicleSpeed_1;//speedometer
    SendDeathMessage(killerid, playerid, reason);
    if (!(Events[playerid][0] || Events[playerid][1] || Events[playerid][2] || Events[playerid][3])) //при смерти на евентах АБНУЛЯИМСЯ)))
    {
        Events[playerid][0] = false;
    }
    if (Checkpoint[playerid] == 1) //дальнобойщик
    {
        DisablePlayerCheckpoint(playerid);
        Checkpoint[playerid] = 0;
        SendClientMessage(playerid, COLOR_VIOLET, ""NS" {FFFF00}Вы умерли и не смогли доставить груз!");
    }//дальнобойщик
    if (Checkpoint[playerid] == 2) //дальнобойщик
    {
        DisablePlayerCheckpoint(playerid);
        Checkpoint[playerid] = 0;
        SendClientMessage(playerid, COLOR_VIOLET, ""NS" {FFFF00}Вы умерли и не смогли доставить груз!");
    }//дальнобойщик

    //DUELSYS
    if (Duel[killerid] >= 2 && Duel[playerid] >= 2)
    {
        if (Duelrounds[playerid] <= 1)
        {
            if (Duel[playerid] >= 2)
            {
                Duel[playerid] = 0;
                SetPlayerVirtualWorld(playerid, 0);
                Gotrequest[playerid] = 0;
                Maderequest[playerid] = 0;
                KillTimer(UnDuelend[playerid]);
            }
        }
        if (Duelrounds[playerid] > 1)
        {
            Duelrounds[playerid] -= 1;
            KillTimer(UnDuelend[playerid]);
        }
        //Killerid
        if (Duelrounds[killerid] <= 1)
        {
            if (Duel[killerid] >= 2)
            {
                Gotrequest[killerid] = 0;
                Maderequest[killerid] = 0;
                Duelwins[killerid] += 1;
                SetPlayerVirtualWorld(killerid, 0);
                Duel[killerid] = 0;
                new string2[256];
                new Weaponname[32], Weaponname2[32];
                GetWeaponName(Duelweap1[playerid], Weaponname, 32);
                GetWeaponName(Duelweap2[playerid], Weaponname2, 32);
                format(string, sizeof(string), "[DUEL]: {FFFFFF}%s {CCFF00}победил в дуэле с {FFFFFF}%s.{CCFF00} Оружие: {FFFFFF}%s, %s {CCFF00}Ставка: {FFFFFF}%d$", PlayerInfo[killerid][pName], PlayerInfo[playerid][pName], Weaponname, Weaponname2, Duelbet[playerid]);
                format(string2, sizeof(string2), "[DUEL]: {FFFFFF}Финальный результат: {CCFF00}%s {99FF99}%i:%i {CCFF00}%s", PlayerInfo[killerid][pName], Duelwins[killerid], Duelwins[playerid], PlayerInfo[playerid][pName]);
                SendClientMessageToAll(COLOR_VIOLET, string);
                SendClientMessageToAll(COLOR_VIOLET, string2);
                SendClientMessage(killerid, COLOR_VIOLET, "[DUEL]: {FFFFFF}Вы выйграли в дуэле и получили награду! Поздравляем!!!");
                MoneyK(killerid, Duelbet[playerid]);
                MoneyK(playerid, -Duelbet[playerid]);
                //WeaponBugDuel[killerid] = 1;
                //WeaponBugDuel[playerid] = 1;
                Duelbet[playerid] = 0;
                Duelbet[killerid] = 0;
                ResetPlayerWeapons(killerid);
                ResetPlayerWeapons(playerid);
                KillTimer(UnDuelend[killerid]);
                SpawnPlayer(killerid);
            }
        }
    }
    if (Duel[killerid] >= 2 && Duel[playerid] < 2)
    {
        if (Gotrequest[killerid] == 1)
        {
            new PID = Duelmaker[killerid];
            Gotrequest[killerid] = 0;
            Maderequest[PID] = 0;
            SetPlayerVirtualWorld(killerid, 0);
            Duel[killerid] = 0;
            Duelrounds[killerid] = 0;
            SetPlayerVirtualWorld(PID, 0);
            Duel[PID] = 0;
            Duelrounds[PID] = 0;
            SendClientMessage(killerid, COLOR_VIOLET, "[DUEL]: {FFFFFF}Дуэль завершилась из-за многих ограничений");
            SendClientMessage(PID, COLOR_VIOLET, "[DUEL]: {FFFFFF}Дуэль завершилась из-за многих ограничений");
            //WeaponBugDuel[killerid] = 1;
            //WeaponBugDuel[PID] = 1;
            Duelbet[PID] = 0;
            Duelbet[killerid] = 0;
            ResetPlayerWeapons(PID);
            ResetPlayerWeapons(killerid);
            KillTimer(UnDuelend[killerid]);
            KillTimer(UnDuelend[PID]);
            SpawnPlayer(PID);
            SpawnPlayer(killerid);
        }
        if (Maderequest[killerid] == 1)
        {
            new PID = Duelopponent[killerid];
            Maderequest[killerid] = 0;
            Gotrequest[PID] = 0;
            SetPlayerVirtualWorld(killerid, 0);
            Duel[killerid] = 0;
            Duelrounds[killerid] = 0;
            SetPlayerVirtualWorld(PID, 0);
            Duel[playerid] = 0;
            Duelrounds[PID] = 0;
            SendClientMessage(killerid, COLOR_VIOLET, "[DUEL]: {FFFFFF}Дуэль завершилась из-за многих ограничений");
            SendClientMessage(PID, COLOR_VIOLET, "[DUEL]: {FFFFFF}Дуэль завершилась из-за многих ограничений");
            //WeaponBugDuel[killerid] = 1;
            //WeaponBugDuel[PID] = 1;
            Duelbet[killerid] = 0;
            Duelbet[PID] = 0;
            ResetPlayerWeapons(PID);
            ResetPlayerWeapons(killerid);
            KillTimer(UnDuelend[killerid]);
            KillTimer(UnDuelend[PID]);
            SpawnPlayer(PID);
            SpawnPlayer(killerid);
        }
    }
    if (Duel[killerid] < 2 && Duel[playerid] >= 2)
    {
        if (Gotrequest[playerid] == 1)
        {
            new PID = Duelmaker[playerid];
            Gotrequest[playerid] = 0;
            Maderequest[PID] = 0;
            SetPlayerVirtualWorld(playerid, 0);
            Duel[playerid] = 0;
            Duelrounds[playerid] = 0;
            SetPlayerVirtualWorld(PID, 0);
            Duel[PID] = 0;
            Duelrounds[PID] = 0;
            SendClientMessage(playerid, COLOR_VIOLET, "[DUEL]: {FFFFFF}Дуэль завершилась из-за многих ограничений");
            SendClientMessage(PID, COLOR_VIOLET, "[DUEL]: {FFFFFF}Дуэль завершилась из-за многих ограничений");
            //WeaponBugDuel[PID] = 1;
            //WeaponBugDuel[playerid] = 1;
            Duelbet[playerid] = 0;
            Duelbet[PID] = 0;
            ResetPlayerWeapons(PID);
            ResetPlayerWeapons(playerid);
            KillTimer(UnDuelend[playerid]);
            KillTimer(UnDuelend[PID]);
            SpawnPlayer(PID);
            SpawnPlayer(playerid);
        }
        if (Maderequest[playerid] == 1)
        {
            new PID = Duelopponent[playerid];
            Maderequest[playerid] = 0;
            Gotrequest[PID] = 0;
            SetPlayerVirtualWorld(playerid, 0);
            Duel[playerid] = 0;
            Duelrounds[playerid] = 0;
            SetPlayerVirtualWorld(PID, 0);
            Duel[PID] = 0;
            Duelrounds[PID] = 0;
            SendClientMessage(playerid, COLOR_VIOLET, "[DUEL]: {FFFFFF}Дуэль завершилась из-за многих ограничений");
            SendClientMessage(PID, COLOR_VIOLET, "[DUEL]: {FFFFFF}Дуэль завершилась из-за многих ограничений");
            //WeaponBugDuel[PID] = 1;
            //WeaponBugDuel[playerid] = 1;
            Duelbet[PID] = 0;
            Duelbet[playerid] = 0;
            ResetPlayerWeapons(PID);
            ResetPlayerWeapons(playerid);
            KillTimer(UnDuelend[playerid]);
            KillTimer(UnDuelend[PID]);
            SpawnPlayer(PID);
            SpawnPlayer(playerid);
        }
    }//DUELSYS

    if (killerid != INVALID_PLAYER_ID)
    {
        PlayerInfo[killerid][pKills] += 1;
        PlayerInfo[playerid][pDeaths] += 1;

        if (dmzone[playerid] == 0 && dmzone[killerid] == 0)
        {
            SetPVarInt(killerid, "PlMon", GetPVarInt(killerid, "PlMon") + 5000);
            SendClientMessage(killerid, COLOR_VIOLET, ""NS" {FFFFFF}За убийство {CCFF00}+$5000");
            //for(new i; i < 26; i++) gPlayerWeapon[playerid][i] = 0;
            if (GetPVarInt(playerid, "goodspawn") != 1)
            {
                SendClientMessage(playerid, COLOR_VIOLET, ""NS"{FFFFFF} Режим бессмертия активирован. Чтобы {8F30E4}Отключить {FFFFFF}его, нанесите урон по любому игроку");
                SetPlayerHealth(playerid, 100);
                SetPlayerArmour(playerid, 100);
                SetPVarInt(playerid, "goodspawn", 1);
            }
        }

        if (dmzone[playerid] > 0 && dmzone[killerid] > 0) //если смерть и убийство в DM-зоне, то:
        {
            dmkills[killerid]++;//число убийств игрока +1
            dmdeath[playerid]++;//число смертей игрока +1

            format(string, sizeof(string), "Вы получили 10000$ за убийство %s на ДМ! (Убийств: %d / Смертей: %d)", PlayerInfo[playerid][pName], dmkills[killerid], dmdeath[killerid]);
            SendClientMessage(killerid, COLOR_VIOLET, string);
            format(string, sizeof(string), "Вас убил %s на ДМ! (Убийств: %d / Смертей: %d)", PlayerInfo[killerid][pName], dmkills[playerid], dmdeath[playerid]);
            SendClientMessage(playerid, COLOR_VIOLET, string);
            SetPVarInt(killerid, "PlMon", GetPVarInt(killerid, "PlMon") + 10000);
            SetPlayerHealth(killerid, 100);
        }

    }
    return 1;
}

