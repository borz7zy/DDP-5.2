{
    if (AI[playerid][aSpectateID] != INVALID_PLAYER_ID) return 1;
    if (AI[playerid][aLevel] < 4) return 1;
    new type, cost;
    if (sscanf(params, "dd", type, cost)) return SendClientMessage(playerid, COLOR_VIOLET, "USE: {FFFFFF}/cbus [ид типа бизнеса(/bushelp) (0-19)] [цена]");
