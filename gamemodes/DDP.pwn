        if (BI[i][bID] == 0)
        {
            break;
        }
        else i++;
    }
    if (i == MAX_BUSSINES)
    {
        SendClientMessage(playerid, COLOR_VIOLET, ""NS" {FFFFFF}На сервере достигнут лимит по колличеству бизнесов на карте!");
